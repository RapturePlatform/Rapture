// Test Merge and MergeIf

a = { 'one' : 1 };
b = { 'two' : 2 };

c = mergeif(a, b);

println(c);

a = { 'one' : 1 };
b = { 'one' : 2 };

c = mergeif(a, b);

println(c);

a = { 'inner' : { 'one' : 1 }};
b = { 'inner' : { 'two' : 2 }};

c = mergeif(a,b);

println(c);

a = { 'one' : 1 };
b = { 'one' : { 'inner' : 1 }};

c = mergeif(a,b);
println(c);

a = { 'one' : 1};
b = { 'two' : 2};
c = { 'three' : 3 };

d = mergeif(a,b,c);

println(d);
