//************
// test we can catch an exception thrown by a local function
//************

def exceptFunc ()
	//throw 'testing you can catch this!';
	throw "testing you can catch this!";
end


// set the return value here of false
    retVal = false;

try do
    
	exceptFunc();
    
end
catch e do
	println('caught exception: ' + e);
	retVal = true;
end

return retVal;


